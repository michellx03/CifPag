package br.com.cifpag.entity;

import java.io.Serializable;
import java.sql.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="pedido", schema="sistema")
public class Pedido implements Serializable {
	
	@Id
	@Column(name="pedi_id")
	@GeneratedValue(strategy= GenerationType.IDENTITY)
	private Integer pediId;
	
	@Column(name="pedi_numero_documento")
	private String pediNumeroDocumento;
	
	@Column(name="pedi_data_documento")
	private Date pediDataDocumento;
	
	@Column(name="pedi_numero_documento_fiscal")
	private Integer pediNumeroDocumentoFiscal;pedi_numero_documento
	
	@Column(name="pedi_produto", length=50)
	private String pediProduto;
	
	@Column(name="pedi_quantidade")
	private Double pediQuantidade;
	
	@Column(name="pedi_unidade", length=2)
	private String pediUnidade;
	
	@Column(name="pedi_preco_unitario_produto")
	private Double pediPrecoUnitarioProduto;
	
	@Column(name="")
	private Double pedi_preco_total_produto;
	
	@Column(name="")
	private Double pedi_total_desconto_produto;
	
	@Column(name="")
	private Double pedi_valor_desconto_produto;
	
	@Column(name="")
	private Double pedi_porcentagem_desconto_produto;
	
	@Column(name="")
	private Double pedi_total_peso;
	
	@Column(name="")
	private Integer pedi_quantidade_total;
	
	@Column(name="")
	private Double pedi_desconto_total;
	
	@Column(name="")
	private Double pedi_porcentagem_total;
	
	@Column(name="")
	private Double pedi_valor_total;
	
	@Column(name="")
	private Double pedi_valor_final; 
	
	@Column(name="")
	private Integer pedi_empresa;
	
	@Column(name="")
	private Integer pedi_vendedor; 
	
	@Column(name="")
	private Integer pedi_cliente;
	
	@Column(name="")
	private Integer pedi_operacao;

}
